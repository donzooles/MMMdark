/*
		THE MENU BAR
	*/
QMenuBar {
  background: #212121;
  color: #FFF; }
  
QMenuBar::item {
  background: transparent;
  padding: 6px 12px; }

QMenuBar::item:selected,
QMenuBar::item:pressed {
  background: #424242;
  color: #FFF; }

QMenuBar::item:pressed {
  background: #424242;
  color: #FFF; }

QMenu {
  background: #424242;
  border: 0px solid #424242;
  color: #FFF; }

QMenu::item {
  border: 0px solid transparent;
  color: #FFF;
  padding: 6px 12px;
  padding-left: 25px; }

QMenu::item:selected {
  background: #363636; }

QMenu::item:pressed {
  background: #303030; }

QMenu::item:disabled {
  background: transparent; }

QMenu::separator {
  background: #303030;
  height: 1px; }

QMenu::indicator {
  padding-top: 2px;
  height: 25px;
  width: 25px; }
  
QMenu::indicator {
  width: 12px;
  left: 6px; }

QMenu::indicator:checked {
  image: url(skin:controls/checkbox_checked_dark.svg); }

/*
		CHANGES THE MUMBLE TITLE TO MUMBLE
		AKA HIDE THE UGLY DEV BUILD NUMBER
	*/
QMainWindow#MainWindow {
  qproperty-windowTitle: "Mumble";
  qproperty-frame: false; }

